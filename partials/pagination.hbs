<nav class="pagination" role="pagination">
    {{#if prev}}
        <a class="newer-posts" href="{{page_url prev}}"><span aria-hidden="true">←</span> Newer</a>
    {{/if}}
    {{#if next}}
        <a class="older-posts" href="{{page_url next}}">Older <span aria-hidden="true">→</span></a>
    {{/if}}
</nav>
